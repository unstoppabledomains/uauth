    "release": "../../scripts/release.sh"
  },
  "devDependencies": {
    "@unstoppabledomains/resolution": "^8.0"
  },
  "peerDependencies": {
    "@unstoppabledomains/resolution": "^8.0"
  },
  "publishConfig": {
    "access": "public"

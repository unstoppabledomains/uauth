{
  "name": "@uauth/web3-onboard",
  "version": "2.7.1",
  "type": "module",
  "exports": {
    "import": "./build/index.module.mjs",
    "require": "./build/index.cjs"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.module.mjs",
  "source": "./src/index.ts",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yarn export-version; microbundle",

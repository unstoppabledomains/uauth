{
  "name": "@uauth/node",
  "homepage": "https://github.com/unstoppabledomains/uauth/tree/main/packages/node#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unstoppabledomains/uauth.git",
    "directory": "packages/node"
  },
  "bugs": {
    "url": "https://github.com/unstoppabledomains/uauth/issues"
  },
  "version": "2.4.0",
  "type": "commonjs",
  "exports": {
    "module": "./build/index.module.js",
    "import": "./build/index.module.js",
    "default": "./build/index.js"
  },
  "main": "./build/index.js",
  "unpkg": "./build/index.umd.js",
  "module": "./build/index.module.js",
  "source": "./src/index.ts",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yarn export-version; microbundle --target node",
    "dev": "yarn export-version; microbundle watch --target node",
    "export-version": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > ./src/version.ts",
    "release": "../../scripts/release.sh"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@uauth/common": "workspace:*",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5"
  },
  "devDependencies": {
    "@unstoppabledomains/resolution": "^8.0",
    "express": "^4.17",
    "express-session": "^1.17"
  },
  "peerDependencies": {
    "@unstoppabledomains/resolution": "^8.0",
    "express": "^4.17",
    "express-session": "^1.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "amdName": "UAuthNode"
}
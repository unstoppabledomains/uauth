{
  "name": "@uauth/web3-react",
  "version": "2.4.0",
  "files": [
    "build/*"
  ],
  "type": "commonjs",
  "types": "./build/index.d.ts",
  "main": "./build/index.js",
  "exports": "./build/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "tsc --watch",

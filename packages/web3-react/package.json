  },
  "dependencies": {
    "@uauth/js": "workspace:*",
    "@web3-react/core": "8.0.35-beta.0",
    "@web3-react/eip1193": "8.0.26-beta.0",
    "@web3-react/metamask": "8.0.28-beta.0",
    "@web3-react/network": "8.0.27-beta.0",
    "@web3-react/types": "8.0.20-beta.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "amdName": "UAuthWeb3React"
}
